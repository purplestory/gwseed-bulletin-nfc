{
  "version": 2,
  "builds": [
    {
      "src": "nfc_redirect.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/nfc_redirect.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "nfc_redirect.py": {
      "maxDuration": 30
    }
  }
} 